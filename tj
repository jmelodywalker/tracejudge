#!/usr/bin/env python3

import sys
import subprocess


def main():
    if len(sys.argv) < 3:
        print("Usage: ./tj analyze <path_to_run.json>")
        sys.exit(1)

    command = sys.argv[1]

    if command == "analyze":
        path = sys.argv[2]
        subprocess.run(["python", "analyze.py", path])
    else:
        print(f"Unknown command: {command}")
        sys.exit(1)


if __name__ == "__main__":
    main()